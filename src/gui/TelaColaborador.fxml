<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.layout.RowConstraints?>
<?import javafx.scene.text.Font?>

<AnchorPane xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="gui.ColaboradorrController">
   <children>
      <Pane layoutX="-6.0" prefHeight="400.0" prefWidth="600.0" style="-fx-background-color: #3c3c3c;">
         <children>
            <GridPane layoutY="6.0" prefHeight="400.0" prefWidth="600.0">
               <columnConstraints>
                  <ColumnConstraints halignment="CENTER" hgrow="SOMETIMES" maxWidth="300.0" minWidth="10.0" prefWidth="300.0" />
                  <ColumnConstraints halignment="CENTER" hgrow="SOMETIMES" maxWidth="300.0" minWidth="10.0" prefWidth="300.0" />
               </columnConstraints>
               <rowConstraints>
                  <RowConstraints maxHeight="50.0" minHeight="10.0" prefHeight="50.0" valignment="CENTER" vgrow="SOMETIMES" />
                  <RowConstraints maxHeight="50.0" minHeight="10.0" prefHeight="50.0" valignment="TOP" vgrow="SOMETIMES" />
                  <RowConstraints maxHeight="150.0" minHeight="10.0" prefHeight="150.0" valignment="BOTTOM" vgrow="SOMETIMES" />
                  <RowConstraints maxHeight="50.0" minHeight="10.0" prefHeight="50.0" valignment="BOTTOM" vgrow="SOMETIMES" />
                  <RowConstraints maxHeight="50.0" minHeight="10.0" prefHeight="50.0" valignment="TOP" vgrow="SOMETIMES" />
                  <RowConstraints maxHeight="160.0" minHeight="10.0" prefHeight="160.0" valignment="TOP" vgrow="SOMETIMES" />
               </rowConstraints>
               <children>
                  <Label text="Peças" textFill="WHITE">
                     <font>
                        <Font size="24.0" />
                     </font>
                  </Label>
                  <ComboBox fx:id="comboBoxPecas" prefWidth="150.0" GridPane.rowIndex="1" />
                  <Label text="Clientes" textFill="WHITE" GridPane.columnIndex="1">
                     <font>
                        <Font size="24.0" />
                     </font>
                  </Label>
                  <ComboBox fx:id="comboBoxClientes" prefWidth="150.0" GridPane.columnIndex="1" GridPane.rowIndex="1" />
                  <Label text="Serviços" textFill="WHITE" GridPane.rowIndex="3">
                     <font>
                        <Font size="24.0" />
                     </font>
                  </Label>
                  <Label prefHeight="35.0" prefWidth="217.0" text="Ordem de Serviço" textFill="WHITE" GridPane.columnIndex="1" GridPane.rowIndex="3">
                     <font>
                        <Font size="24.0" />
                     </font>
                  </Label>
                  <ComboBox fx:id="comboBoxServicos" prefWidth="150.0" GridPane.rowIndex="4" />
                  <Button fx:id="btnEmitirOS" mnemonicParsing="false" style="-fx-background-color: #f26500;" text="Emitir" textFill="WHITE" GridPane.columnIndex="1" GridPane.rowIndex="5">
                     <font>
                        <Font size="24.0" />
                     </font>
                  </Button>
                  <Button fx:id="btnPecas" mnemonicParsing="false" onAction="#ProsseguirPecas" prefWidth="100.0" text="Prosseguir" GridPane.rowIndex="2" GridPane.valignment="TOP" />
                  <Button fx:id="btnClientes" mnemonicParsing="false" prefWidth="100.0" text="Prosseguir" GridPane.columnIndex="1" GridPane.rowIndex="2" GridPane.valignment="TOP" />
                  <Button fx:id="btnServicos" mnemonicParsing="false" prefWidth="100.0" text="Prosseguir" GridPane.rowIndex="5" />
               </children>
            </GridPane>
         </children>
      </Pane>
   </children>
</AnchorPane>
